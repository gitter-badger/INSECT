{
  "name": "insect-tool",
  "version": "3.1.6",
  "description": "INcosiderate SEntence CaTcher",
  "main": "cli.js",
  "scripts": {
    "start": "node_modules/.bin/supervisor app",
    "test": "node_modules/.bin/mocha -w",
    "docs:build": "npm run docs:prepare && rm -rf _book && gitbook build",
    "docs:prepare": "gitbook install",
    "docs:watch": "npm run docs:prepare && gitbook serve",
    "docs:publish": "npm run docs:build && cd _book && git init && git commit --allow-empty -m 'Update docs' && git checkout -b gh-pages && git add . && git commit -am 'Update docs' && git push git@github.com:CallyWally12/INSECT gh-pages --force"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CallyWally12/INSECT.git"
  },
  "keywords": [],
  "author": "Callum Leslie",
  "bin": {
    "insect-tool": "bin/cli.js"
  },
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/CallyWally12/INSECT/issues"
  },
  "homepage": "https://github.com/CallyWally12/INSECT#readme",
  "dependencies": {
    "gitbook-plugin-advanced-emoji": "0.1.9",
    "insect_json": "*",
    "meow": "3.7.0",
    "xmlhttprequest": "1.8.0"
  },
  "engines": {
    "node": "6.2.1"
  },
  "devDependencies": {
    "gitbook-cli": "2.3.0",
    "mocha": "*",
    "should": ">= 0.0.1",
    "supervisor": "*"
  },
  "preferGlobal": "true"
}
